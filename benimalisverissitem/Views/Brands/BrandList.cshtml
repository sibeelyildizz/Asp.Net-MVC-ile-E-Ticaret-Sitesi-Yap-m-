@model IEnumerable<benimalisverissitem.Models.Brands>

<div class="well">
    <h4>Markalar</h4>
    <div class="row">
        <div class="list">
            <ul class="list-unstyled">
                @foreach (var item in Model)
                {
                    <li>
                        <input type="checkbox" class="brand-checkbox" name="@item.Marka" value="@item.Id" />
                        @item.Marka
                    </li>
                }
            </ul>

        </div>

    </div>
</div>

