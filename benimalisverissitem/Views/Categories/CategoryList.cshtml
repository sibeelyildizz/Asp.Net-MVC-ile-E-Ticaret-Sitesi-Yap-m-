@model IEnumerable<benimalisverissitem.Models.Categories>


<div class="well">
    <h4>Kategoriler</h4>
    <div class="row">
       
            <div  class="list">
                <ul class="list-unstyled">
                    @foreach (var item in Model)
                    {
                        <li>
                            <input type="checkbox" class="category-checkbox" name="@item.kategoriAdi" value="@item.Id" />
                            @item.kategoriAdi

                        </li>
                    }
                </ul>

            </div>
    </div>

</div>


